page{
  background-color: #F6ECEC;
}
.z-title{
  padding: 10px;
  color: #FFC925;
  text-indent: 12rpx;
  font-family: "楷体";
  font-size: 22px;
  font-weight: bold;
}
.z-word{
  background-color: rgba(255,255,255,0.3);
  padding: 4px;
  border-radius: 50px;
}
.z-photo{
  width: 190px;
  height: 190px;
}
.z-photo image{
  width: 100%;
  height: 100%;
}
.z-major{
  display: block;
  margin-bottom: 12px;
  position: relative;
  background-color: #fff;
}
.z-content{
  width: 180px;
  position:absolute;
  top: 12px;
  right: 18px;
}
.z-content text{
  display: block;
}
.z-t1{
  color: #666;
  margin-bottom: 12px;
}
.z-t2{
  font-size: 16px;
  color: #666;
  font-family: '宋体';
  letter-spacing: 1px;
}
.z-magazine{
  width: 100%;
  height: 180px;
}

.sk-main{
  height:160px;
  width: 100%;
  overflow: hidden;
}
.sk-list{
  width: 170%;
  height:160px;
  display:flex;
}
movable-view view{
  display: inline-block;
}
.zz-pic{
  width: 120px;
  height: 160px;
  margin-left: 10px;
}
.zz-pic image{
  width: 100%;
  height: 100%;
}