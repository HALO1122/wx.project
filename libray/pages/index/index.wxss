/**index.wxss**/
page{
  background-color: #f5f5f5;
}
.yx-photo{
  width: 160px;
  height: 160px;
  border-radius: 50%;
}
.yx-photo image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.yx-msg{
  margin-top: 30px;
  color: #FFC925;
}

.yx-center{
  width: 100%;
  height: 300px;
  margin-top: 20px;
  position: relative;
}
.yx-pic{
  width: 90px;
  height: 90px;
  text-align: center;
  padding: 28px 38px 36px 38px;
}
.p1{
  background-color: #FFDB03;
 
  position: absolute;
  left: 20px;
}
.p2{
  position: absolute;
  right: 10px;
}
.p3{
  position: absolute;
  top: 180px;
  left: 20px;
}
.p4{
  background-color: #FFDB03;
  position: absolute;
  top: 180px;
  right: 10px;
}
.yx-pic image{
  width: 100%;
  height: 100%;
}